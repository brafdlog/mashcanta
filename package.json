{
  "name": "Mashcanta",
  "version": "0.0.1",
  "description": "Mortgage optimizer and calculator",
  "main": "''",
  "scripts": {
    "deploy": "npm run test && npm run build && firebase deploy",
    "build": "./build.sh",
    "build:dev": "webpack --progress --colors",
    "clean": "rm -f public/*hash--*",
    "dev": "webpack-dev-server --port 8181 --progress --profile --colors --hot",
    "test": "NODE_ENV=test mocha ./src/test/**/*.js --compilers js:babel-register",
    "test:watch": "NODE_ENV=test mocha ./src/test/**/*.js --compilers js:babel-register --watch",
    "test:coverage": "nyc npm test",
    "generateSvgSprite": "svg-sprite-generate -d icons -o svgSprite.svg"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/brafdlog/mashcanta"
  },
  "keywords": [],
  "author": "Jonathan Goldfarb",
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "bootstrap": "^3.3.7",
    "bootstrap-rtl": "^3.3.4",
    "chart.js": "^2.2.1",
    "classnames": "^2.2.5",
    "firebase": "^3.3.0",
    "lodash": "^4.13.1",
    "mobx": "^2.5.1",
    "mobx-react": "^3.5.6",
    "react": "15.3.0",
    "react-chartjs-2": "^1.2.3",
    "react-dom": "15.3.0",
    "react-select": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "babel-core": "6.7.7",
    "babel-loader": "6.2.4",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "css-loader": "0.23.1",
    "eslint": "^2.11.0",
    "eslint-config-canonical": "^1.7.9",
    "eslint-config-canonical-lodash": "^1.0.18",
    "eslint-config-canonical-react": "^1.2.16",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-objects": "^1.1.1",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "^2.22.0",
    "jsx-loader": "^0.13.2",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "nyc": "^8.3.0",
